{
    "name": "Meet Minutes in Minutes",
    "description": "A chrome extension that summarises a meeting.",
    "version": "0.0.1",
    "manifest_version": 3,
    "action": {
        "default_popup":"popup.html",
        "default_icon": "logo.png"
    },
    "icons":{
      "16": "logo.png",
      "48": "logo.png",
      "128": "logo.png"
    },
    "permissions": ["activeTab", "storage","microphone"]
  
  }